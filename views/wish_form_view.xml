<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="wish_form_view">
            <field name="name">wish.form</field>
            <field name="model">offer_stats.wish</field>
            <field name="arch" type="xml">
                <form string="Wish Form">
                    <sheet>
                        <group>
                            <field name="wish_id"/>
                            <field name="software_id"/>
                            <field name="min_price"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>
